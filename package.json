{
  "name": "mock-config-inspector",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "dev:app": "vite",
    "build:app": "vite build",
    "build:swc": "swc bin src index.ts -d dist --extensions .ts --copy-files --ignore **/*.test.ts",
    "build": "yarn build:app && yarn build:swc",
    "lint": "eslint . --fix",
    "type": "tsc --noEmit",
    "format": "prettier --write .",
    "pretty": "yarn type && yarn lint && yarn format",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "express": "^4.21.1",
    "lucide-react": "^0.462.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@siberiacancode/eslint": "^2.6.0",
    "@siberiacancode/prettier": "^1.1.1",
    "@siberiacancode/stylelint": "^1.1.1",
    "@swc/cli": "^0.5.1",
    "@swc/core": "^1.9.3",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1"
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix"
    ],
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
